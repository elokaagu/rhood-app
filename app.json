{
  "expo": {
    "name": "R/HOOD",
    "slug": "rhoodapp",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/rhood_logo.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rhoodapp.mobile",
      "buildNumber": "70",
      "newArchEnabled": false,
      "usesAppleSignIn": true,
      "googleServicesFile": "./ios/GoogleService-Info.plist",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "audio"
        ],
        "NSCameraUsageDescription": "This app needs access to camera to take photos for messages",
        "NSPhotoLibraryUsageDescription": "This app needs access to photo library to select images and videos for messages",
        "NSMicrophoneUsageDescription": "This app needs access to microphone to record audio for messages",
        "NSUserNotificationsUsageDescription": "This app needs notification permissions to show music controls on the lock screen",
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to show nearby opportunities and calculate distances.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location to show nearby opportunities and calculate distances.",
        "NSUserTrackingUsageDescription": "We use your data to improve your experience and show you relevant opportunities.",
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "rhoodapp",
            "CFBundleURLSchemes": [
              "rhoodapp"
            ]
          }
        ],
        "LSApplicationQueriesSchemes": [
          "mailto"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/rhood_logo.png",
        "backgroundColor": "#000000"
      },
      "package": "com.rhoodapp.mobile",
      "googleServicesFile": "./android/app/google-services.json",
      "newArchEnabled": false,
      "permissions": [
        "android.permission.WAKE_LOCK",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.RECORD_AUDIO",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.FOREGROUND_SERVICE_MEDIA_PLAYBACK",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.CAMERA",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ]
    },
    "notification": {
      "icon": "./assets/rhood_logo.png",
      "color": "#ffffff"
    },
    "extra": {
      "eas": {
        "projectId": "499f2244-43f3-478d-8b88-85c2da9f1d52"
      },
      "openaiModel": "gpt-4o-mini",
      "mixpanelToken": "93ccc1f355ea98a1b4a34565320f6a83"
    },
    "plugins": [
      "expo-font",
      "expo-asset",
      "expo-av",
      "expo-web-browser",
      "expo-video",
      [
        "expo-notifications",
        {
          "icon": "./assets/rhood_logo.png",
          "color": "#ffffff",
          "iosDisplayInForeground": true
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "This app needs access to your location to show nearby opportunities and calculate distances."
        }
      ],
      [
        "expo-tracking-transparency",
        {
          "userTrackingPermission": "We use your data to improve your experience and show you relevant opportunities."
        }
      ],
      "./plugins/withFirebaseModularHeaders",
      "@react-native-firebase/app",
      "expo-asset"
    ],
    "scheme": "rhoodapp",
    "owner": "elokaagu",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/499f2244-43f3-478d-8b88-85c2da9f1d52"
    }
  }
}
